    "prepare": "bob build",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "document-scanner",
    "scanner",
    "ocr",
    "document",
    "scan",
    "camera",
    "visionkit",
    "mlkit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangthien123/react-native-document-scanner.git"
  },
  "author": "Hoang Thien <hoangthien123@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hoangthien123/react-native-document-scanner/issues"
  },
  "homepage": "https://github.com/hoangthien123/react-native-document-scanner#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.74.0",
    "@types/react": "^18.2.44",
    "@types/react-native": "^0.72.8",
    "eslint": "^8.19.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-native": "^0.74.0",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "^5.3.3"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
